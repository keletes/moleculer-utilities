{
	"compilerOptions": {
		"target": "es2021",
		"module": "commonjs",
		"strict": true,
		"noEmitOnError": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"esModuleInterop": true,
		"declaration": true
	},
	"include": [
		"index.ts",
		"middlewares",
		"mixins"
	],
	"exclude": [
		"node_modules"
	]
}
